{
    "name": "doc",
    "private": false,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "docs:dev": "pnpm run register:components && vitepress dev docs --host",
        "docs:build": "pnpm run register:components && cross-env BASE=/beeboat-plus/ vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "register:components": "vitepress-rc"
    },
    "dependencies": {
        "axios": "^0.26.1",
        "pinia": "2.0.26",
        "vue": "^3.2.45",
        "element-plus": "2.2.22"
    },
    "devDependencies": {
        "@element-plus/icons-vue": "2.0.10",
        "@vitejs/plugin-vue": "3",
        "cross-env": "7.0.3",
        "prettier": "2.7.1",
        "sass": "1.56.1",
        "vite": "3.2.4",
        "vitepress": "1.0.0-alpha.28",
        "vitepress-theme-demoblock": "2.0.0",
        "vue-router": "4.1.6",
        "vue-tsc": "^1.0.9"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@algolia/client-search"
            ]
        }
    }
}