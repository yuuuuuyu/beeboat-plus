{
  "name": "@beeboat/example",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docs:dev": "pnpm run register:components && vitepress dev docs --host",
    "docs:build": "pnpm run register:components && cross-env BASE=/beeboat-plus/ vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^0.26.1",
    "cross-env": "^7.0.3",
    "element-plus": "2.7.6",
    "pinia": "^2.0.18",
    "vue": "^3.2.37",
    "vue-cookies": "^1.8.2",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "markdown-it-container": "^3.0.0",
    "sass": "^1.51.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vite-plugin-dts": "^1.7.0",
    "vitepress": "^1.0.0-alpha.28",
    "vitepress-theme-demoblock": "^2.0.0",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^0.34.7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}